<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>https://zupple.fr/</loc>
    </url>
    <url>
        <loc>https://zupple.fr/lyon/</loc>
    </url>
    {% for story in $indexes.storiesByArea["lyon"] %}
        <url>
            <loc>https://zupple.fr/{{ story.fullSlug }}</loc>
        </url>
    {% endfor %}
    {% for universe in $indexes.universesByArea["lyon"] %}
        <url>
            <loc>https://zupple.fr/{{ universe.fullSlug }}</loc>
        </url>
    {% endfor %}
    <url>
        <loc>https://zupple.fr/tarifs/</loc>
    </url>
    <url>
        <loc>https://zupple.fr/news/</loc>
    </url>
    {% for news in $indexes.allNews.all %}
        <url>
            <loc>https://zupple.fr/{{ news.fullSlug }}</loc>
        </url>
    {% endfor %}

    {% for enigme in $indexes.allVendredinigmes['all'] %}
        <url>
            <loc>https://zupple.fr/{{ enigme.fullSlug }}</loc>
        </url>
    {% endfor %}
    <url>
        <loc>https://zupple.fr/faq/</loc>
    </url>
    {% for topic, faqs in $indexes.faqByTopic %}
        {% for faq in faqs %}
            <url>
                <loc>https://zupple.fr/{{ faq.fullSlug }}</loc>
            </url>
        {% endfor %}
    {% endfor %}
    <url>
        <loc>https://zupple.fr/newsletter/</loc>
    </url>
</urlset>
